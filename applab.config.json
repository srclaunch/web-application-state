{
  "build": [
    {
      "buildDir": "dist",
      "buildFile": "index.js",
      "buildTypes": false,
      "bundle": true,
      "codeSplitting": true,
      "excludeLibs": [
        "@srclaunch/web-environment",
        "@srclaunch/exceptions",
        "@srclaunch/i18n",
        "@srclaunch/logger",
        "@srclaunch/http-services",
        "@srclaunch/themes",
        "@srclaunch/types",
        "@srclaunch/validation",
        "amazon-cognito-identity-js",
        "aws-sdk",
        "redux",
        "react",
        "react-dom",
        "react-redux",
        "react-router",
        "react-router-dom"
      ],
      "define": {},
      "format": "esm",
      "inputScripts": ["src/index.tsx"],
      "minify": true,
      "outDir": "dist",
      "platform": "browser",
      "sourceMap": true,
      "target": "es2020",
      "treeShaking": true
    },
    {
      "buildDir": "dist",
      "buildFile": "index.cjs.js",
      "buildTypes": false,
      "bundle": true,
      "codeSplitting": false,
      "excludeLibs": [
        "@srclaunch/web-environment",
        "@srclaunch/exceptions",
        "@srclaunch/i18n",
        "@srclaunch/logger",
        "@srclaunch/http-services",
        "@srclaunch/themes",
        "@srclaunch/types",
        "@srclaunch/validation",
        "amazon-cognito-identity-js",
        "redux",
        "aws-sdk",
        "react",
        "react-dom",
        "react-redux",
        "react-router",
        "react-router-dom"
      ],
      "define": {},
      "format": "cjs",
      "inputScripts": ["src/index.tsx"],
      "minify": true,
      "platform": "browser",
      "sourceMap": true,
      "target": "es2015",
      "treeShaking": true
    }
  ]
}
