{
  "name": "@srclaunch/web-application-state",
  "description": "Redux state and utilities used by AppLab web applications",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.54",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build:types": "tsc --build --force",
    "build:vite": "vite build",
    "build": "yarn clean && yarn build:vite && yarn build:types",
    "clean": "rm -fr ./dist",
    "lint:eslint": "yarn eslint ./src --ext js,ts,tsx --max-warnings 0",
    "lint:stylelint": "yarn stylelint '**/*.ts[x]'",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "push": "git push --follow-tags origin main",
    "release": "yarn standard-version && git push --follow-tags origin main",
    "test": "yarn jest",
    "test:functional": "yarn cypress open",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage",
    "test:coverage:typescript": "yarn typescript-coverage-report",
    "version:push": "git push --follow-tags origin main",
    "version:bump": "yarn version patch --force",
    "version:release": "yarn standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "@srclaunch/exceptions": "^0.1.6",
    "@srclaunch/http-client": "^0.1.2",
    "@srclaunch/i18n": "^0.1.2",
    "@srclaunch/logger": "^0.1.2",
    "@srclaunch/themes": "^0.1.15",
    "@srclaunch/validation": "^0.1.2",
    "@srclaunch/web-environment": "^0.1.7",
    "amazon-cognito-identity-js": "^5.2.8",
    "crypto-js": "^4.1.1",
    "history": "5.2.0",
    "luxon": "^2.3.0",
    "nanoid": "^3.2.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.18",
    "@srclaunch/dx": "^0.1.15",
    "@srclaunch/types": "^0.1.21",
    "@types/history": "4.7.11",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.2.0",
    "babel-plugin-styled-components": "^2.0.6",
    "esbuild": "^0.14.19",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux-logger": "^3.0.6",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.6"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
